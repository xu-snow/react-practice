{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es2015",
    "mapRoot": "compiled",
    "inlineSourceMap":true,
    "outFile": "./compiled/**/*.js"
  }
}